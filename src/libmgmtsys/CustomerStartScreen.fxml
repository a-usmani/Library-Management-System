        </columns>
      </TableView>
      <Button fx:id = "B" layoutX="14.0" layoutY="318.0" mnemonicParsing="true" onAction="#btnBuyClicked" prefHeight="58.0" prefWidth="175.0" text="_Buy" />
      <Button fx:id="BandR" layoutX="204.0" layoutY="318.0" mnemonicParsing="true" onAction="#btnBuyAndRedeemClicked" prefHeight="58.0" prefWidth="193.0" text="_Redeem Points and Buy" />
      <Button fx:id = "Logout" layoutX="410.0" layoutY="318.0" mnemonicParsing="true" onAction="#btnLogoutClicked" prefHeight="58.0" prefWidth="175.0" text="_Logout" />
